{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "boilerplate for a react project",
  "main": "./scripts/start.js",
  "scripts": {
    "afterpull": "npm i && npm prune",
    "start": "babel-node --stage 0 ./scripts/start",
    "prebuild": "babel-node --stage 0 ./scripts/clean",
    "build": "babel-node --stage 0 ./scripts/build",
    "build-prod": "babel-node --stage 0 ./scripts/build --minify",
    "test": "npm run build && testem"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bloodyowl/react-boilerplate.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "webpack"
  ],
  "author": "bloodyowl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bloodyowl/react-boilerplate/issues"
  },
  "homepage": "https://github.com/bloodyowl/react-boilerplate",
  "devDependencies": {
    "appcache-webpack-plugin": "^0.1.1",
    "babel": "^5.0.12",
    "babel-core": "^5.0.12",
    "babel-loader": "^5.0.0",
    "babel-runtime": "^5.0.12",
    "bloody-react-styled": "^2.0.2",
    "classnames": "^1.1.4",
    "css-loader": "^0.9.1",
    "cssnext-loader": "^1.0.1",
    "es5-shim": "^4.1.1",
    "file-loader": "^0.8.1",
    "jsdom": "3.1.2",
    "markdown-loader": "^0.1.2",
    "object-assign": "^2.0.0",
    "opn": "^1.0.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.2.3",
    "rimraf": "^2.3.2",
    "style-loader": "^0.8.3",
    "svgo": "^0.5.0",
    "svgo-loader": "^1.1.0",
    "tape": "^4.0.0",
    "tape-catch": "^1.0.4",
    "testem": "^0.7.5",
    "webpack": "^1.8.4",
    "webpack-dev-server": "^1.7.0"
  },
  "dependencies": {
    "cssrecipes-custom-media-queries": "^0.3.0",
    "cssrecipes-defaults": "^0.5.0",
    "cssrecipes-grid": "^0.4.0",
    "cssrecipes-reset": "^0.5.0",
    "cssrecipes-utils": "^0.5.0",
    "normalize.css": "^3.0.2",
    "react": "^0.13.0",
    "react-router": "^0.13.1",
    "whatwg-fetch": "^0.7.0"
  }
}
